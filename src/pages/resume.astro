                                dateEnded={item.dateEnded}
                                location={item.location}
                                duties={item.duties} lastOne={false} />}
            {(arr.length-1 == i) &&
            <ExperienceCentered name={item.name}
                                company={item.company}
                                dateStarted={item.dateStarted}
                                dateEnded={item.dateEnded}
                                location={item.location}
                                duties={item.duties} lastOne={true} />}
            </>  
                    
        ))}
    </section>
</section>


